R1(config)#line con 0
R1(config-line)#password cisco
R1(config-line)#login

R1(config)#line vty 0 15
R1(config-line)#password cisco
R1(config-line)#login
R1(config-line)#end
R1(config)#int gi0/0/1
R1(config-if)#ip add 192.168.1.1 255.255.255.0
R1(config-if)#no shut
